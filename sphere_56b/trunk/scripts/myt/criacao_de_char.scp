[FUNCTION criacao_de_personagem]
src.allskills=0
src.str=10
src.dex=10
src.int=10
src.findlayer(12).remove
src.findlayer(21).remove
src.findlayer(1).remove
src.findlayer(2).remove
src.findlayer(layer_hair).remove

src.newitem=i_backpack
src.act.equip

src.newitem=i_adaga
src.act.equip

if !(<restest 1 i_candle>)
 src.newitem=i_candle
 src.act.equip
endif

src.newitem=i_coin_copper //cooper coins
src.act.amount=300
src.act.bounce

src.dialog d_criacaomyt
return 1

