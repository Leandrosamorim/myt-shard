[FUNCTION peixe]
if (<src.targ.distance> > 2) && (<src.targ.topobj.uid> != <src.uid>)
  src.sysmessage Chegue mais perto.
return 1
elif (<src.targ.baseid> == i_fish_big_1)||(<src.targ.baseid> == i_fish_big_2)||(<src.targ.baseid> == i_fish_big_3)||(<src.targ.baseid> == i_fish_big_4)
	if (<src.targ.topobj.uid> == <src.uid>)
	  src.newitem=i_fish_raw
	  src.act.bounce
	  src.newitem=i_fish_head
	  src.act.bounce
		if (<src.targ.amount>>1)
		  src.targ.amount=<src.targ.amount>+(-1)
		  src.targ.update
		return 1
		elif (<src.targ.amount>==1)
		  src.targ.remove
		return 1
		endif
	return 1
	else
	  src.newitem=i_fish_raw
	  src.act.p=<src.targ.p>
	  src.act.move 0 1 0
	  src.newitem=i_fish_head
	  src.act.p=<src.targ.p>
	  src.act.move 1 0 0
	  src.act.timer={60 80]
		if (<src.targ.amount>>1)
		  src.targ.amount=<src.targ.amount>+(-1)
		  src.targ.update
		return 1
		elif (<src.targ.amount>==1)
		  src.targ.remove
		return 1
		endif
	return 1
	endif
return 1
elif (<src.targ.baseid> == i_fish_small)
	if (<src.targ.topobj.uid> == <src.uid>)
	  src.newitem=i_fish_raw_small
	  src.act.amount=4
	  src.act.bounce
		if (<src.targ.amount>>1)
		  src.targ.amount=<src.targ.amount>+(-1)
		  src.targ.update
		return 1
		elif (<src.targ.amount>==1)
		  src.targ.remove
		return 1
		endif
	return 1
	else
	  src.newitem=i_fish_raw_small
	  src.act.amount=4
	  src.act.p=<src.targ.p>
	  src.act.move 1 0 0
		if (<src.targ.amount>>1)
		  src.targ.amount=<src.targ.amount>+(-1)
		  src.targ.update
		return 1
		elif (<src.targ.amount>==1)
		  src.targ.remove
