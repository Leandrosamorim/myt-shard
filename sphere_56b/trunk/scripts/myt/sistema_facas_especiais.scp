//////////// funções ///////////////////////
///////////////////////////////////////////

[FUNCTION sangrar]
	sfx 059
	src.targ.morey=(<src.targ.morey>|01)
dorand 5
	src.newitem=0122a
	src.newitem=0122b
	src.newitem=0122d
	src.newitem=0122e
	src.newitem=0122f
enddo
	src.act.attr=02
	src.act.p=<src.targ.p>
	src.act.timer={60 80]
dorand 5
	src.newitem=0122a
	src.newitem=0122b
	src.newitem=0122d
	src.newitem=0122e
	src.newitem=0122f
enddo
	src.act.attr=02
	src.act.p=<src.targ.p>
	src.act.move 1 0 0
	src.act.timer={60 80]
	var.srctarg=<src.targ.uid>
	src.anim 020
	src.message *tirando a pele do ser morto*
	src.targ.color=020
return 1

