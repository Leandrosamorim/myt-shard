[ITEMDEF i_faca_pelar]  //Skinning kinife - usada para tirar o couro dos seres
NAME=Faca para pelar
id=0ec4
TYPE=T_WEAPON_FENCE
RESOURCES=2 i_ingot_iron
DAM=1,10
//SPEED=80
SKILL=Swordsmanship
REQSTR=5
TWOHANDS=N
WEIGHT=1
CATEGORY=Myt itens (Jean Gabin)
SUBSECTION=Facas
DESCRIPTION=Faca para pelar

ON=@Create
HITPOINTS={31 40}

on=@dclick
target Em que voce deseja usar isso?
return 1

ON=@Targon_ground 
 SRC.SYSMESSAGE Use esta faca apenas para tirar o couro ou o pelo de seres mortos.
RETURN 1  

on=@targon_char
 SRC.SYSMESSAGE Use esta faca apenas para tirar o couro ou o pelo de seres mortos.
return 1

ON=@Targon_item 
if (<src.targ.type>!=t_corpse)
 SRC.SYSMESSAGE Use esta faca apenas para tirar o couro ou o pelo de seres mortos.
return 1
else
	src.pelar
endif
return 1


