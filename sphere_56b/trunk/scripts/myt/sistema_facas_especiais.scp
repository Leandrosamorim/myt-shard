[ITEMDEF 013f6]
defname=i_faca_corte
name=Faca de acogueiro
WEIGHT=2
TYPE=T_WEAPON_SWORD
FLIP=1
DAM=2,8
//SPEED=80
SKILL=Swordsmanship
REQSTR=5
TWOHANDS=N
DUPELIST=013f7

CATEGORY=Myt itens (Jean Gabin)
SUBSECTION=Facas
DESCRIPTION=Faca de acougueiro

ON=@Create
HITPOINTS={31 40}

on=@dclick
target Em que voce deseja usar isso?
return 1

ON=@Targon_ground 
 SRC.SYSMESSAGE Use esta faca apenas para extrair carne e orgaos de seres mortos.
RETURN 1  

on=@targon_char
 SRC.SYSMESSAGE Use esta faca apenas para extrair carne e orgaos de seres mortos.
return 1

ON=@Targon_item 
if (<src.targ.type>==t_fish)
	src.peixe
return 1
elif (<src.targ.topobj.uid> == <src.uid>)
	src.sysmessage coloque o corpo em uma mesa ou no chão para poder extrair a carne e os orgaos.
return 1
elif (<src.targ.type>==t_corpse)
	src.acogueiro
endif
return 1


