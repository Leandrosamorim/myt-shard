[FUNCTION habilitacao_buttons_cr]
//limpa a tag resource e coloca os botões para não selecionados. 
if (!strmatch(<src.tag.resource_change_resource>,)	
	src.tag.bt_carp1=<src.tag.bt_carp1>
	src.tag.bt_carp2=<src.tag.bt_carp2>
	src.tag.bt_carp3=<src.tag.bt_carp3>
	src.tag.bt_carp4=<src.tag.bt_carp4>
	src.tag.bt_carp5=<src.tag.bt_carp5>
	src.tag.bt_carp6=<src.tag.bt_carp6>
	src.tag.bt_carp7=<src.tag.bt_carp7>
	src.tag.bt_carp8=<src.tag.bt_carp8>
	return 1
else
	src.tag.bt_carp1=00D2
	src.tag.bt_carp2=00D2
	src.tag.bt_carp3=00D2
	src.tag.bt_carp4=00D2
	src.tag.bt_carp5=00D2
	src.tag.bt_carp6=00D2
	src.tag.bt_carp7=00D2
	src.tag.bt_carp8=00D2
	return 1
endif
return 1

