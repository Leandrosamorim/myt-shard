//*****************************************************************************
// cr_fazTabua
//*****************************************************************************
[FUNCTION cr_fazTabua]
//if (!<src.resource_change>)
//     return 1
//endif
obj=<src.findid.i_mry_fazitem.uid>
obj.cleartags
obj.tag.skill=lumberjacking

//obj.tag.res1=i_log<src.tag.resource_change.sufixtype>
//obj.tag.nam1=tabua%s<src.tag.resource_change.sufixname>
//obj.tag.item=i_tabua<src.tag.resource_change.sufixtype>
//obj.tag.name=tabua%s<src.tag.resource_change.sufixname>
obj.tag.res1=i_log
obj.tag.nam1=tora
obj.tag.item=i_tabua
obj.tag.name=tabua%s

obj.tag.amt1=1
obj.tag.dif=20
obj.tag.anim=9
obj.tag.sfx=023d
obj.tag.delay=10
obj.tag.emote=trabalhando
obj.tag.espid=i_racha
obj.tag.espname=racha
obj.tag.scrap=i_foligem_carp
obj.tag.amt=<eval <src.lumberjacking>/100>
src.fazitem
return 1

