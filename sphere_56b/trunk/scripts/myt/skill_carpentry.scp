[DIALOG d_bs_fazitem BUTTON]

on=
	src.dialog=d_bs_fazitem
 	src.update
	return 
on=0					//INCREMENTA
	if ( <src.tag.qtditem> < 10 )
		src.tag.qtditem=<src.tag.qtditem>+1
		src.dialog=d_bs_fazitem
 		src.update
		return 
	else
		src.sysmessageyellow Limite alcancado
		src.dialog=d_bs_fazitem
 		src.update
		return 
on=1					//DECREMENTA
	if ( <src.tag.qtditem> >0 )	
		src.tag.qtditem=<src.tag.qtditem>-1
		src.update
 		src.dialog=d_bs_fazitem
		return 
	else
		src.sysmessageyellow Limite alcancado
		src.dialog=d_bs_fazitem
		src.update
		return 
on=2					// ZERA O CONTADOR
	src.tag.qtditem=0 
	src.update
 	src.dialog=d_bs_fazitem
	return 
on=3					//FAZ O ITEM
	if (<src.tag.qtditem> == 0 )
		src.sysmessageyellow Nao foi selecionada a quantidade
		return
	else
		src.fazitem <src.tag.qtditem>
		src.update	
		return 
on=4
	src.tag.qtditeml=<src.tag.qtditem>
	src.tag.qtditem=
	return
on=5  
	src.tag.qtditem=10
	src.dialog=d_bs_fazitem
	src.update
	return




