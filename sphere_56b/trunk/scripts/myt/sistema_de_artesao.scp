 remove
 return 1
else
 if (<cont.targ.distance> > 1)
  cont.sysmessage Chegue mais perto.
  remove
  return 1
 else
  more2=3
  cont.freeze
  cont.say *transcrevendo*
  cont.consume 1 i_documento_branco
  timer=2
  return 1
 endif
endif

on=@timer
if (<more2>!=1)
 more2=(<more2>-1)
 timer=2
 DORAND (2)
  sfx 019
  cont.say *transcrevendo*
 ENDDO
 return 1
else
 cont.unfreeze
 cont.tentarfalhar_insc
 cont.newitem=<cont.tag.projeto>
 cont.act.cont=<cont.findlayer(21).uid>
 cont.DoGastaTinta
 return 1
endif
return 1

[FUNCTION  tentarfalhar_insc]
