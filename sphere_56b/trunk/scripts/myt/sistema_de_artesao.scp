[ITEMDEF i_mry_fazitemtk]
ID=i_memory
type=t_eq_script
name=Fazendo item tinker
layer=30

on=@dclick
if (<cont.tag.tk_needtype>!=0)
 TARGET
 return 1
else
 timer=1
 cont.consume <eval 0<cont.tag.tk_amt1>> <cont.tag.tk_res1>
 cont.consume <eval 0<cont.tag.tk_amt2>> <cont.tag.tk_res2>
 cont.consume <eval 0<cont.tag.tk_amt3>> <cont.tag.tk_res3>
 cont.consume <eval 0<cont.tag.tk_amt4>> <cont.tag.tk_res4>
return 1
endif


on=@TARGON_CHAR
cont.sysmessage Alvo invalido.
cont.unfreeze
remove
return 1

on=@TARGON_GROUND
cont.sysmessage Alvo invalido.
cont.unfreeze
return 1

on=@TARGON_ITEM
if (<cont.targ.type>==<cont.tag.tk_needtype>)
 if (<cont.targ.distance> >= 2)
  cont.sysmessage Chegue mais perto.
  cont.unfreeze
  remove
  return 1
 endif
 timer=1
 cont.consume <eval 0<cont.tag.tk_amt1>> <cont.tag.tk_res1>
 cont.consume <eval 0<cont.tag.tk_amt2>> <cont.tag.tk_res2>
 cont.consume <eval 0<cont.tag.tk_amt3>> <cont.tag.tk_res3>
 cont.consume <eval 0<cont.tag.tk_amt4>> <cont.tag.tk_res4>
 return 1
endif

on=@timer
cont.freeze
if (<more1><=1)
 cont.unfreeze
 cont.tentarfalhar_tink <eval <cont.tag.tk_dif>>
 cont.newitem=<cont.tag.tk_item>
 cont.act.cont=<cont.findlayer(21).uid>
 if (<eval 0<cont.tag.tk_iamt>> >1)
  cont.act.amount=((<eval 0<cont.act.amount>>-1) + <eval 0<cont.tag.tk_iamt>>)
 endif
 cont.act.update
 cont.cleartagstk
 remove
else
 more1=<more1>-1
 timer=1
 DORAND (4)
  cont.say *trabalhando*
  cont.anim <eval 0<cont.tag.tk_anim>>
  sfx <cont.tag.tk_sfx>
 ENDDO
endif
return 1


[FUNCTION tentarfalhar_tink]
if (<tinkering> <= {<argn> 1051})
