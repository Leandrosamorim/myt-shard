[FUNCTION fazitemtk]
if (<tag.tk_espid>!=0) && !(<src.restest 1 <tag.tk_espid>>)
 src.sysmessage Voce precisa de:
 src.sysmessage - <tag.tk_espname>
 src.sysmessage - <eval 0<tag.tk_amt1>> <tag.tk_nam1>
 if (<tag.tk_res2>!=0)
  src.sysmessage - <eval 0<tag.tk_amt2>> <tag.tk_nam2>
 endif
 if (<tag.tk_res3>!=0)
  src.sysmessage - <eval 0<tag.tk_amt3>> <tag.tk_nam3>
 endif
 if (<tag.tk_res4>!=0)
  src.sysmessage - <eval 0<tag.tk_amt4>> <tag.tk_nam4>
 endif
 return 1
endif 


if !(<src.restest <eval 0<tag.tk_amt1>> <tag.tk_res1>>)
 src.sysmessage Voce precisa de:
 src.sysmessage - <eval 0<tag.tk_amt1>> <tag.tk_nam1>
 if (<tag.tk_res2>!=0) && (<restest <eval 0<tag.tk_amt2>> <tag.tk_res2>>==0)
  src.sysmessage - <eval 0<tag.tk_amt2>> <tag.tk_nam2>
 endif
 if (<tag.tk_res3>!=0) && (<restest <eval 0<tag.tk_amt3>> <tag.tk_res3>>==0)
