 return 1
endif
src.newitem=i_mry_transctk
src.act.more1=<eval <argn>>
src.act.equip
src.act.use
return 1

[ITEMDEF i_mry_transctk]
NAME=transcrevendo projeto tinker
ID=i_memory
TYPE=t_eq_script
LAYER=30

on=@dclick
TARGET selecione a escrivanhinha.
return 1

on=@Targon_Char
cont.sysmessage Isso nao e uma escrivaninha.
remove
return 1

on=@Targon_Ground
cont.sysmessage Isso nao e uma escrivaninha.
remove
return 1

on=@Targon_Item
